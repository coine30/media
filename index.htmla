      --main-shadow: 0 8px 38px 0 #06b6d422, 0 1.5px 8px #06b6d433, 0 8px 32px #11182744;
    }
    body {
      margin: 0;
      font-family: 'Poppins', sans-serif;
      color: var(--main-accent);
      text-align: center;
      padding: 20px;
      min-height: 100vh;
      background: #111827 url('https://images.unsplash.com/photo-1506744038136-46273834b3fb?auto=format&fit=crop&w=1500&q=80') no-repeat center center fixed;
      background-size: cover;
      background-blend-mode: darken;
      position: relative;
    }
    body::before {
      content: "";
      position: fixed;
      top:0;left:0;right:0;bottom:0;
      background: linear-gradient(120deg, #111827 78%, #06b6d411 100%);
      z-index: 0;
      pointer-events: none;
    }
    .header {
      font-size: 2.3rem;
      font-weight: 700;
      color: var(--main-accent);
      background: linear-gradient(110deg, #181f2f 60%, #67e8f9 120%);
      padding: 27px 22px 20px 22px;
      border-radius: 28px 28px 14px 14px;
      margin-top: 55px;
      border: 2.5px solid var(--main-accent);
      letter-spacing: 0.85px;
      box-shadow: var(--main-shadow);
      text-shadow: 0 2px 18px #06b6d488, 0 1px 0 #f9fafb40;
      font-family: 'Poppins', sans-serif;
      transform-style: preserve-3d;
      position: relative;
      z-index: 2;
      backdrop-filter: blur(3px);
    }
    .header:hover {
      transform: translateY(-7px) scale(1.045) rotateX(7deg) skewY(-1.5deg);
      box-shadow: 0 18px 54px 0 #06b6d4cc, 0 1px 0 #fff;
      transition: 0.4s cubic-bezier(.42,.78,.34,.98);
    }
    .countdown {
      margin-top: 16px;
      font-size: 19px;
      font-weight: bold;
      color: var(--main-accent);
      animation: countdownBlink 2s infinite;
      letter-spacing: 1.13px;
      background: #192132cc;
      display: inline-block;
      padding: 9px 26px;
      border-radius: 32px;
      border: 1.7px solid var(--main-accent);
      box-shadow: 0 2px 14px #06b6d422, 0 1px 6px #67e8f944;
      margin-bottom: 10px;
      backdrop-filter: blur(2px);
    }
    .products-grid {
      display: flex;
      flex-direction: column;
      gap: 34px;
      align-items: center;
      margin-top: 36px;
      position: relative;
      z-index: 1;
    }
    .container {
      max-width: 430px;
      background: linear-gradient(120deg, var(--main-card) 85%, var(--main-card2) 100%);
      padding: 38px 34px 32px 34px;
      border-radius: 0 0 28px 28px;
      box-shadow: 0 22px 62px 0 #06b6d411, 0 5px 16px #67e8f944, 0 2px 9px #06b6d433;
      border: 2.3px solid var(--main-accent);
      position: relative;
      animation: fadeSlideIn 1s cubic-bezier(.42,.78,.34,.98) both;
      transition: background 0.4s, box-shadow 0.4s, border 0.2s;
      transform-style: preserve-3d;
      perspective: 600px;
      z-index: 2;
      backdrop-filter: blur(7px);
    }
    .container:hover {
      transform: translateY(-8px) scale(1.035) rotateY(-5deg) skewX(1deg);
      box-shadow: 0 32px 64px #06b6d444, 0 8px 32px #67e8f944;
