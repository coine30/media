<script>
  function getMidnightTimestamp() {
    const now = new Date();
    const midnight = new Date();
    midnight.setHours(24, 0, 0, 0);
    return midnight.getTime();
  }
  function startCountdown() {
    const timers = document.querySelectorAll('.timer');
    const targetTime = getMidnightTimestamp();
    const interval = setInterval(() => {
      const now = new Date().getTime();
      const diff = targetTime - now;
      if (diff <= 0) {
        clearInterval(interval);
        timers.forEach(el => el.textContent = "00:00:00");
        return;
      }
      const hours = String(Math.floor(diff / (1000 * 60 * 60))).padStart(2, '0');
      const minutes = String(Math.floor((diff % (1000 * 60 * 60)) / (1000 * 60))).padStart(2, '0');
      const seconds = String(Math.floor((diff % (1000 * 60)) / 1000)).padStart(2, '0');
      timers.forEach(el => el.textContent = `${hours}:${minutes}:${seconds}`);
    }, 1000);
  }
  function handlePurchase() {
    const button = document.querySelector('.pay-button');
    const stripeUrl = "https://ademo46.github.io/crypto-payment/";
    document.getElementById('unlock-sound').play();
    button.textContent = "Processing...";
    button.disabled = true;
    setTimeout(() => {
      window.location.href = stripeUrl;
    }, 1500);
  }
  startCountdown();
  window.addEventListener('load', () => {
    const button = document.querySelector('.pay-button');
    const urlParams = new URLSearchParams(window.location.search);
    const success = urlParams.get("success");
    if (button) {
      button.disabled = false;
      button.textContent = "üõ°Ô∏è Get Instant Access";
      if (success === "true") {
        document.getElementById('unlock-sound').play();
        setTimeout(() => {
          window.location.href = "https://drive.google.com/uc?export=download&id=1KSURn3iRVrcQwYVIMoIObERqNUsyJdCx";
        }, 1000);
      }
    }
  });
</script>
